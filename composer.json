{
    "name": "mervick/yii2-adminlte",
    "description": "Yii2 Advanced Application With The AdminLTE Theme On The Backend",
    "keywords": ["yii2", "framework", "advanced", "adminlte", "project template"],
    "homepage": "https://github.com/mervick/yii2-adminlte",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/mervick/yii2-adminlte"
    },
    "authors": [
        {
            "name": "Andrey Izman",
            "email": "izmanw@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-faker": "*",
        "mervick/yii2-adminlte-debug": "*",
        "mervick/yii2-adminlte-gii": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
